<!DOCTYPE EstrellaDAM [
        <!ELEMENT EstrellaDAM (carrera)*>
        <!ELEMENT carrera (vuelta,pilotos)*>       
        <!ELEMENT vuelta (#PCDATA)>       
        <!ELEMENT pilotos (piloto)*>
        <!ELEMENT piloto (nombre,puesto,intervalo,coche)*>       
        <!ELEMENT nombre (#PCDATA)>
        <!ELEMENT puesto (#PCDATA)>     
        <!ELEMENT intervalo (#PCDATA)>
        <!ELEMENT coche (numcoche,modelo,Infocoche)*>        
        <!ELEMENT numcoche (#PCDATA)>
        <!ELEMENT modelo (#PCDATA)>
        <!ELEMENT Infocoche (velocidad,bateria,marcha,RPM)*>
        <!ELEMENT velocidad (#PCDATA)>
        <!ELEMENT bateria (#PCDATA)>
        <!ELEMENT marcha (#PCDATA)>
        <!ELEMENT RPM (#PCDATA)>
        <!ATTLIST carrera campeonato CDATA #REQUIRED>
        <!ATTLIST vuelta num CDATA #REQUIRED>
        <!ATTLIST piloto id CDATA #REQUIRED>
        <!ATTLIST puesto posc CDATA #REQUIRED>
        <!ATTLIST coche id CDATA #REQUIRED>
        ]>